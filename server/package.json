{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "introspect": "prisma introspect --schema=prisma/schema.prisma",
    "create-db": "NODE_PATH=./dist node dist/init/create-db.js",
    "init-db": "NODE_PATH=./dist npm run create-db & npm run populate-db",
    "populate-db": "NODE_PATH=./dist node dist/init/populate-db.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"cyan.bold,yellow.bold\"  \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "NODE_PATH=./dist nodemon dist/index.js",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/pg": "^7.14.3",
    "@types/validator": "^13.0.0",
    "array-flatten": "^3.0.0",
    "async-redis": "^1.1.7",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.1.0",
    "prisma": "^1.34.10",
    "typescript": "^3.8.3",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0",
    "@types/async-redis": "^1.1.0",
    "@types/node": "^14.0.13",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
